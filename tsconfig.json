        "public/js/core/application-run.ts",
        "public/js/directives/navbar/navigation-bar-directive.ts",
        "public/js/controllers/blog-controller.ts",
        "public/js/controllers/blog-details-controller.ts",
        "public/js/controllers/home-controller.ts"
    ]
}